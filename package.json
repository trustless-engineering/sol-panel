{
	"name": "sol-panel",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"check-format": "pnpm prettier --check .",
		"check-lint": "pnpm eslint --ext .tsx,.ts .",
		"check-types": "tsc --pretty --noEmit",
		"db:migrate": "prisma migrate deploy",
		"db:reset": "prisma migrate reset --force",
		"db:seed": "prisma db seed",
		"dev:repl": "ts-node -r tsconfig-paths/register ./src/utils/repl.ts",
		"dev": "prisma generate && next dev",
		"format": "prettier --write .",
		"lint": "next lint",
		"prepare": "husky install",
		"start:prod": "pnpm run build && pnpm run start",
		"start": "next start",
		"test": "jest",
		"test-all": "pnpm run check-format && pnpm run check-lint && pnpm run check-types && pnpm run build",
		"worker:webhooks": "ts-node -r tsconfig-paths/register ./src/executors/webhooks.ts"
	},
	"dependencies": {
		"@headlessui/react": "^1.7.15",
		"@hookform/resolvers": "^3.1.1",
		"@kubernetes/client-node": "^0.18.1",
		"@metaplex-foundation/js": "^0.19.4",
		"@prisma/client": "^4.16.2",
		"@solana/web3.js": "^1.77.3",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.9",
		"@tippyjs/react": "^4.2.6",
		"@typescript-eslint/parser": "^5.60.1",
		"axios": "^1.4.0",
		"classnames": "^2.3.2",
		"daisyui": "^3.1.7",
		"date-fns": "^2.30.0",
		"dayjs": "^1.11.9",
		"ethers": "^5.7.2",
		"faktory-worker": "^4.5.1",
		"human-readable": "^0.2.1",
		"jotai": "^1.13.1",
		"js-cookie": "^3.0.5",
		"js-yaml": "^4.1.0",
		"json-to-pretty-yaml": "^1.2.2",
		"little-state-machine": "^4.8.0",
		"lodash": "^4.17.21",
		"next": "13.4.0",
		"next-pwa": "^5.6.0",
		"next-seo": "^5.15.0",
		"next-sitemap": "^3.1.55",
		"next-themes": "^0.2.1",
		"overlayscrollbars": "^2.2.0",
		"overlayscrollbars-react": "^0.5.0",
		"parse-prometheus-text-format": "^1.1.1",
		"rc-slider": "^10.2.1",
		"react": "18.2.0",
		"react-countdown": "^2.3.5",
		"react-dom": "18.2.0",
		"react-dropzone": "^14.2.3",
		"react-hook-form": "^7.45.1",
		"react-icons": "^4.10.1",
		"react-query": "^3.39.3",
		"react-share": "^4.4.1",
		"react-syntax-highlighter": "^15.5.0",
		"react-table": "^7.8.0",
		"react-toastify": "^9.1.3",
		"react-use": "^17.4.0",
		"react-use-websocket": "^4.3.1",
		"recharts": "^2.7.2",
		"redis": "^4.6.7",
		"swr": "^2.2.0",
		"tiny-invariant": "^1.3.1",
		"uuid": "^9.0.0",
		"ws": "^8.13.0",
		"yaml-loader": "^0.8.0",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@babel/core": "^7.22.5",
		"@babel/preset-env": "^7.22.5",
		"@babel/preset-typescript": "^7.22.5",
		"@jest/types": "^29.5.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/jest": "^29.5.2",
		"@types/js-cookie": "^3.0.3",
		"@types/js-yaml": "^4.0.5",
		"@types/node": "20.2.5",
		"@types/overlayscrollbars": "^1.12.1",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.4",
		"@types/react-syntax-highlighter": "^15.5.7",
		"@types/react-table": "^7.7.14",
		"@types/testing-library__jest-dom": "^5.14.6",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"autoprefixer": "^10.4.14",
		"babel-jest": "^29.5.0",
		"cypress": "^12.16.0",
		"dotenv": "^16.3.1",
		"eslint": "^8.44.0",
		"eslint-config-next": "^13.4.7",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-standard-with-typescript": "^36.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^16.0.1",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"husky": "^8.0.3",
		"jest": "^29.5.0",
		"lint-staged": "^13.2.3",
		"postcss": "^8.4.24",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"prisma": "^4.16.2",
		"rimraf": "^3.0.2",
		"semantic-release": "^21.0.6",
		"semantic-release-helm3": "^2.6.0",
		"tailwindcss": "^3.3.2",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.6"
	},
	"engines": {
		"node": ">=v20.2.0"
	},
	"packageManager": "^pnpm@8.6.1",
	"release": {
		"branches": [
			"main",
			{
				"name": "next",
				"prerelease": true
			}
		],
		"plugins": [
			"@semantic-release/commit-analyzer",
			"@semantic-release/release-notes-generator",
			[
				"@semantic-release/github",
				{
					"assets": [
						{
							"path": "*.tgz",
							"label": "Helm Chart"
						}
					]
				}
			]
		]
	},
	"ts-node": {
		"require": [
			"tsconfig-paths/register"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "pnpm run test-all"
		}
	},
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{css,scss,less}": [
			"prettier --write"
		],
		"*.{json,md,yml,yaml}": [
			"prettier --write"
		]
	}
}
