{
  "name": "sol-panel",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm prisma generate && pnpm next dev",
    "build": "pnpm next build",
    "start": "pnpm next start",
    "start:prod": "pnpm run build && pnpm run start",
    "lint": "pnpm next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "pnpm run check-format && pnpm run check-lint && pnpm run check-types && pnpm run build",
    "prepare": "husky install",
    "db:migrate": "prisma migrate deploy",
    "worker:webhooks": "ts-node -r tsconfig-paths/register ./src/executors/webhooks.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@hookform/resolvers": "^3.1.1",
    "@kubernetes/client-node": "^0.18.1",
    "@metaplex-foundation/js": "^0.19.4",
    "@prisma/client": "^4.16.1",
    "@solana/web3.js": "^1.77.3",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tippyjs/react": "^4.2.6",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "daisyui": "^3.1.6",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.8",
    "ethers": "^5.7.2",
    "faktory-worker": "^4.5.1",
    "formik": "^2.4.2",
    "framer-motion": "^10.12.17",
    "human-readable": "^0.2.1",
    "jotai": "^1.13.1",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "json-to-pretty-yaml": "^1.2.2",
    "little-state-machine": "^4.8.0",
    "lodash": "^4.17.21",
    "next": "13.4.0",
    "next-pwa": "^5.6.0",
    "next-seo": "^5.15.0",
    "next-sitemap": "^3.1.55",
    "next-themes": "^0.2.1",
    "overlayscrollbars": "^2.2.0",
    "overlayscrollbars-react": "^0.5.0",
    "parse-prometheus-text-format": "^1.1.1",
    "rc-slider": "^10.2.1",
    "react": "18.2.0",
    "react-countdown": "^2.3.5",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.45.0",
    "react-icons": "^4.10.1",
    "react-query": "^3.39.3",
    "react-share": "^4.4.1",
    "react-table": "^7.8.0",
    "react-toastify": "^9.1.3",
    "react-use": "^17.4.0",
    "react-use-websocket": "^4.3.1",
    "recharts": "^2.7.2",
    "redis": "^4.6.7",
    "swiper": "^9.4.1",
    "swr": "^2.2.0",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "web3modal": "^1.9.12",
    "ws": "^8.13.0",
    "yaml-loader": "^0.8.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/js-cookie": "^3.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.2.5",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/react-table": "^7.7.14",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "autoprefixer": "^10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "^8.43.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.16.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.3.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=18.14.2"
  },
  "packageManager": "pnpm@6.14.1",
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ]
  },
  "ts-node": {
    "require": [
      "tsconfig-paths/register"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "pnpm run test-all"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,less}": [
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  }
}
